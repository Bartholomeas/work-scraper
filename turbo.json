{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build",
        "clean",
        "^build"
      ]
    },
    "lint": {
      "outputs": []
    },
    "type-check": {
      "outputs": [],
      "dependsOn": [
        "^type-check"
      ]
    },
    "test": {},
    "start": {}
  }
}