{
  "name": "frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "imports": {
    "@/*": "./src/*"
  },
  "exports": {
    "@/*": "./src/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "start": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint --fix",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json,css,scss,html}\""
  },
  "dependencies": {
    "@radix-icons/vue": "^1.0.0",
    "@unovis/ts": "^1.4.3",
    "@unovis/vue": "^1.4.3",
    "@vee-validate/zod": "^4.13.0",
    "@vueuse/core": "^10.11.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.11",
    "lucide-vue-next": "^0.383.0",
    "radix-vue": "^1.8.3",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vee-validate": "^4.13.0",
    "vue": "^3.4.21",
    "vue-query": "^1.26.0",
    "vue-router": "^4.3.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "config": "workspace:*",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "shared": "workspace:*",
    "tailwindcss": "^3.4.3",
    "tsconfig": "workspace:*",
    "typescript": "^5.5.4",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6"
  }
