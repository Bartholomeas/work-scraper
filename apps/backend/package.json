{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "main": "./index.js",
  "scripts": {
    "build": "NODE_ENV=development && tsup --tsconfig tsconfig.json --format cjs,esm --dts",
    "dev": "NODE_ENV=development && tsx watch src/server.ts",
    "start": "node dist/server.js",
    "lint": "eslint --fix --cache 'src/**/*.{ts,tsx}'",
    "type-check": "tsc --noEmit",
    "format": "eslint src/**/*.ts --fix",
    "db:push": "NODE_ENV=development && dotenv -e .env.development -- pnpx prisma db push",
    "db:push:test": "NODE_ENV=test && dotenv -e .env.test -- pnpx prisma db push",
    "db:studio": "NODE_ENV=development && dotenv -e .env.development -- pnpx prisma studio",
    "db:studio:test": "NODE_ENV=test && dotenv -e .env.test -- pnpx prisma studio",
    "postinstall": "pnpx prisma generate",
    "seed": "NODE_ENV=development && dotenv -e .env.development -- ts-node ./prisma/seed.ts",
    "seed:dev": "NODE_ENV=development && ts-node ./prisma/seed.ts",
    "seed:test": "NODE_ENV=test && dotenv -e .env.test -- ts-node ./prisma/seed.ts",
    "reset:db:test": "NODE_ENV=test && dotenv -e .env.test -- pnpx prisma migrate reset",
    "test": "NODE_ENV=test && dotenv -e .env.test -- jest"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "puppeteer": "^22.9.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "slugify": "^1.6.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.2",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.2.0",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "prettier": "^3.2.5",
    "prisma": "^5.16.2",
    "shared": "workspace:*",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig": "workspace:*",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.1.0",
    "tsx": "^4.15.7",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0"
  }
}
