{
  "name": "work-scrapper",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:front": "turbo run dev --filter=frontend",
    "dev:back": "turbo run dev --filter=backend",
    "start": "turbo run start",
    "start:frontend": "turbo run start --filter=frontend",
    "start:backend": "turbo run start --filter=backend",
    "build": "turbo run build",
    "lint": "turbo run lint -- --fix",
    "type-check": "turbo run type-check",
    "test": "turbo run test --filter=backend",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "turbo": "^2.0.9"
  },
  "engines": {
    "npm": ">=10.5.2",
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.1.4+sha512.9df9cf27c91715646c7d675d1c9c8e41f6fce88246f1318c1aa6a1ed1aeb3c4f032fcdf4ba63cc69c4fe6d634279176b5358727d8f2cc1e65b65f43ce2f8bfb0"
}
